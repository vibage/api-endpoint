{
  "name": "api-endpoint",
  "version": "1.0.0",
  "description": "A REST backend for vibage that handles all simple requests",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "gcp-build": "yarn install && npm run build",
    "start:prod": "npm run build && NODE_ENV=prod npm run start",
    "start:dev": "yarn install && npm-run-all --parallel watch:server watch:build",
    "watch:server": "NODE_ENV=dev nodemon dist/server.js | bunyan -o short",
    "watch:build": "tsc -w",
    "deploy": "gcloud app deploy"
  },
  "engines": {
    "node": "10.x.x",
    "npm": "6.x"
  },
  "author": "Tyler Tracy",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/firestore": "^1.2.0",
    "@types/dotenv": "^6.1.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "cookie-parser": "^1.4.4",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.15",
    "node-fetch": "^2.3.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bunyan": "^1.8.5",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.9",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.19",
    "@types/node-fetch": "^2.1.6",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.7",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  }
}